{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "defaults/**/*"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "chorus",
  "mainBinaryName": "chorus",
  "version": "0.0.0",
  "identifier": "com.chorus.app",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "title": "Chorus",
        "decorations": false,
        "maximizable": true,
        "resizable": true,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}